<h1>Activities</h1>

<div id="activities">
  <% @activities.each do |activity| %>
    <%= link_to category_activity_path(category_id: @category.id, id: activity.id) do %>
      <%= render activity %>
    <% end %>
  <% end %>
</div>

<%= link_to "New activity", new_category_activity_path(category_id: @category.id) %>
